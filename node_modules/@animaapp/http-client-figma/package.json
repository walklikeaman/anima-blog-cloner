{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "@animaapp/http-client-figma",
  "version": "1.0.3",
  "description": "Anima's HTTP client for Figma REST API",
  "author": "Anima App, Inc.",
  "license": "ISC",
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "vite build",
    "tests": "vitest run --dir ./tests",
    "deploy": "yarn run build && npm publish --access public --no-workspaces"
  },
  "engines": {
    "node": ">=18.x.x"
  },
  "dependencies": {
    "csvtojson": "^2.0.10",
    "deepcopy": "^2.1.0",
    "eventsource": "^3.0.2",
    "md5": "^2.3.0"
  },
  "devDependencies": {
    "@figma/rest-api-spec": "^0.23.0",
    "@types/md5": "^2.3.5",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.4.2",
    "vite": "^6.0.3",
    "vite-plugin-dts": "^4.0.2",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^2.1.8"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.17.2"
  },
  "files": [
    "dist",
    "package.json",
    "README.md"
  ]
}
